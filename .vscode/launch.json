   "version": "0.2.0",
   "configurations": [
        {
            "name": "Home",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Akkamart/src/Akkamart.Home/Akkamart.Home.Server/bin/Debug/netcoreapp3.0/Akkamart.Home.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Akkamart/src/Akkamart.Home/Akkamart.Home.Server",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                "SQLSERVER_CONNECTION": "Server=localhost,1433;Initial Catalog=Akkamart;Persist Security Info=False;User ID=sa;Password=<YourStrong!Passw0rd>;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;"
                // "SEED_NODE_PORT": "5050",
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Membership",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Akkamart/src/Akkamart.Membership/Akkamart.Membership.Server/bin/Debug/netcoreapp3.0/Akkamart.Membership.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Akkamart/src/Akkamart.Membership/Akkamart.Membership.Server",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                "SQLSERVER_CONNECTION": "Server=localhost,1433;Initial Catalog=Akkamart;Persist Security Info=False;User ID=sa;Password=<YourStrong!Passw0rd>;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;"
                // "SEED_NODE_PORT": "5050",
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Seed1",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Akkamart/src/Akkamart.Seed1/bin/Debug/netcoreapp3.0/Akkamart.Seed1.dll",
            "args": [
                "akkamart.seed1.conf"
            ],
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                "SQLSERVER_CONNECTION": "Server=localhost,1433;Initial Catalog=Akkamart;Persist Security Info=False;User ID=sa;Password=<YourStrong!Passw0rd>;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;"
